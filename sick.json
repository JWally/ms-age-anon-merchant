{
  "scan": ".",

  "include": ["bin", "lib", "src-site", "src-lambda", "test"],

  "exclude": ["node_modules", ".git", "dist", "build", "cdk.out", "coverage"],

  "excludeFiles": ["package-lock.json"],

  "extensions": [".ts", ".tsx", ".json", ".md"],

  "showExcluded": true,
  "showSkipped": true,

  "outputFile": "output.sick"
}
